# FairCircles Backend Environment Variables
# ==========================================
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=development

# FairScale API Configuration
FAIRSCALE_API_URL=https://api.fairscale.xyz
FAIRSCALE_API_KEY=your_fairscale_api_key_here

# Solana Configuration
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
PROGRAM_ID=your_program_id_here

# CORS Configuration (comma-separated list of allowed origins)
# Include your production frontend URL here for deployment
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:3000,https://faircircle.vercel.app
